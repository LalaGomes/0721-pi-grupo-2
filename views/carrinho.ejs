    <main>
        <div id="carrinho">
            <h3>
                 Produto Interno > Carrinho De Compras
            </h3>
           

            <div id="quadro_tabela">
                <table>
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Descrição</th>
                            <th>Quantidade</th>
                            <th>Preço</th>
